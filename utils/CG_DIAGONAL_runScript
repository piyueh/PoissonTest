#! /bin/bash

rm CG_DIAGONAL_log.out

echo "[1;31;47m1 proc, 1 thread[m" >>CG_DIAGONAL_log.out
mpiexec -np 1 -bind-to core -map-by socket:PE=1 -mca hwloc_base_mem_bind_failure_action silent ../build/TriPoisson_MpiThreads --kokkos-ndevices=1 --kokkos-threads=1 --Nx=1000 --solverName=CG --precName=DIAGONAL >>CG_DIAGONAL_log.out
echo " ">>CG_DIAGONAL_log.out

echo "[1;31;47m1 proc, 2 thread[m" >>CG_DIAGONAL_log.out
